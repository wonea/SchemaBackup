<Window x:Class="SchemaBackup.AdminApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Closed="Window_Closed" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <ToolBar>
            <Button ToolTip="Open Configuration" Name="openButton">Open</Button>
            <Button ToolTip="Deploy Configuration" Name="deployButton">Deploy</Button>
            <Button ToolTip="About" Name="aboutButton" Click="aboutButton_Click">About</Button>
        </ToolBar>
        <Grid Grid.Row="1">
            
        </Grid>
    </Grid>
</Window>
