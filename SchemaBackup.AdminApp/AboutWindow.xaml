<Window x:Class="SchemaBackup.AdminApp.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AboutWindow" Height="129" Width="297" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Label>Schema Backup 2014 by Gareth Brown</Label>
        <StackPanel Grid.Row="1" Style="{StaticResource windowStackPanelStyle}">
            <Button Name="cancelButton" 
                    Style="{StaticResource standardCloseButtonStyle}" Click="cancelButton_Click" />
        </StackPanel>
    </Grid>
</Window>
